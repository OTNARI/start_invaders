{
  "name": "Start Invaders",
  "description": "Override the new tab page",
  "version": "0.1",
  "icons": {
    "16": "icons/sp16.png",
    "48": "icons/sp48.png",
    "128": "icons/sp128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "https://api.darksky.net/forecast/*",
    "unlimitedStorage"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
